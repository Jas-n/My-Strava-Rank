ErrorDocument 403 /error
ErrorDocument 404 /error
ErrorDocument 405 /error
ErrorDocument 408 /error
ErrorDocument 500 /error
ErrorDocument 502 /error
ErrorDocument 504 /error

RewriteEngine On
RewriteBase /

# New MSR



# Old MSR
RewriteCond	%{REQUEST_FILENAME}.php -f
RewriteRule	^([A-Za-z\-_]+)/?$	$1\.php	[NC,L]

RewriteCond	%{REQUEST_FILENAME} !-f
RewriteRule ^([A-Za-z\-_]+)/([A-Za-z0-9]+)/?$	index\.php?file=$1&id=$2	[NC,L]
RewriteRule	^([A-Za-z\-_]+)/?$					index\.php?file=$1			[NC,L]



